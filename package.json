    "eslint-plugin-react": "^7.1.0",
    "glob": "^7.1.2",
    "postcss-loader": "^2.0.6",
    "react-devtools": "^2.5.0",
    "style-loader": "^0.18.2",
    "url": "^0.11.0",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "ava": "^0.22.0",
    "clipboard-copy": "^1.2.0",
    "emotion": "^7.2.2",
