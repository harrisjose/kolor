{
    "env": {
        "development": {
            "presets": [
              ["es2015", {"loose": true, "modules": false}],
              "stage-0",
              "react",
              "react-hmre"
            ]
        }
    },
    "plugins": [
      "transform-runtime",
      "syntax-jsx",
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
}
