{
  "presets": [
    ["es2015", {"loose": true, "modules": false}],
    "stage-0",
    "react"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "plugins": [
    "emotion",
    "transform-runtime",
    "syntax-jsx",
    "syntax-object-rest-spread",
    "transform-object-rest-spread"
  ]
}
